{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "watch": "nodemon ./bin/www",
    "tunnel": "ngrok http 3000 & node getNgrokUrl.js",
    "kill": "lsof -t -i:3000",
    "dev": "npm run kill && concurrently --kill-others \"npm run watch\" \"npm run tunnel\""
  },
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "jade": "~1.11.0",
    "json2csv": "^4.1.2",
    "knex": "^0.14.6",
    "morgan": "~1.9.0",
    "objection": "^1.1.8",
    "pg": "^7.4.3"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "node-fetch": "^2.1.2",
    "nodemon": "^1.17.4"
  }
}
